<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- TREE VIEW -->
        <record id="todo_owl_tree_view" model="ir.ui.view">
            <field name="name">todo.tree.view</field>
            <field name="model">todo.list</field>
            <field name="arch" type="xml">
                <tree string="">

                </tree>
            </field>
        </record>

        <!-- FORM VIEW -->
        <record id="model_name_view_form" model="ir.ui.view">
            <field name="name">model_name.view.form</field>
            <field name="model">model_name</field>
            <field name="arch" type="xml">
                <form string="">
                    <sheet>
                        <group>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- ACTIONS-->
        <record id="model_name_action" model="ir.actions.act_window">
            <field name="name">Action Name</field>
            <field name="res_model">model_name</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">

            </field>
        </record>
        <!-- MENU -->
        <menuitem id="model_name_menu" name="name" sequence="10"/>
    </data>
</odoo>
